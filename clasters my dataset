      "source": [
        "# Токенизация, чистка\n",
        "def tokenize(msg):\n",
        "    # убрали знаки препинания и привели к нижнему регистру\n",
        "    msg = re.sub('[!\"?/\\\\().:;,-]', '', msg).lower()\n",
        "    msg = [w for w in word_tokenize(msg) if not w in stopset] # токенизировали и избавились от стоп слов\n",
        "    return msg"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
